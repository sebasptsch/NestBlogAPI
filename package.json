  "scripts": {
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "redis:test:rm": "docker compose rm test-redis -s -f -v",
    "redis:test:up": "docker compose up test-redis -d",
    "redis:dev:rm": "docker compose rm dev-redis -s -f -v",
    "redis:dev:up": "docker compose up dev-redis -d",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 1 && yarn prisma:dev:deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 1 && yarn prisma:test:deploy",
    "redis:test:restart": "yarn redis:test:rm && yarn redis:test:up",
    "redis:dev:restart": "yarn redis:dev:rm && yarn redis:dev:up",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "studio:test": "dotenv -e .env.test -- prisma studio",
    "studio:dev": "dotenv -e .env -- prisma studio"
  },
